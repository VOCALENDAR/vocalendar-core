<h2><%= t ".title" %></h2>

<table class="table table-striped table-hover">
  <tr>
    <th><%= User.human_attribute_name :id %></th>
    <th><%= User.human_attribute_name :name %></th>
    <th><%= User.human_attribute_name :google_account %></th>
    <th><%= User.human_attribute_name :twitter_nick %></th>
    <th><%= User.human_attribute_name :role %></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.id, user %></td>
    <td><%= link_to user.name, user %></td>
    <td><%= user.google_account %></td>
    <td><%= user.twitter_nick %></td>
    <td><%= user.role %></td>
  </tr>
<% end %>
</table>
