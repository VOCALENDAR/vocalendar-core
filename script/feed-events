status = 0

Calendar.where(:io_type => 'src').each do |cal|
  begin
    cal.sync_events
  rescue => e
    msg = "Failed to sync calendar '#{cal.name}' (##{cal.id}): #{e.class.name}: #{e.to_s} (#{e.backtrace.first})"
    Rails.logger.error msg
    $stderr.puts e
    status = 1
  end
end
exit status

